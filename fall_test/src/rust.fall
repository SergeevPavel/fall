tokenizer {
  lparen '('
  rparen ')'
  lbrace '{'
  rbrace '}'
  kw_pub 'pub'
  struct 'struct'
  fn 'fn'
  whitespace r"\s+"
  ident r"\w+"
}

pub rule file { <rep {fn_or_struct} { 'pub' | 'fn' | 'struct' } > }
rule fn_or_struct { fn_def | struct_def }

pub rule fn_def {
  <opt {'pub'}> 'fn' <commit> ident
  '(' ')' '{' '}'
}

pub rule struct_def {
  <opt {'pub'}> 'struct' <commit> ident
  '{' '}'
}
